@model Potato.ViewModels.Account.UserViewModel


@{
    ViewData["Title"] = Model?.User?.FullName ?? "Potato";
}

@if (Model != null)
{
    @await Html.PartialAsync("_Profile", Model)

}
@if (TempData["Notification"] != null)
{
    <script>
        window.onload = function() {
            alert(@Html.Raw(Json.Serialize(TempData["Notification"])));
        };
    </script>
}
